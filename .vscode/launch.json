            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cs/bin/Debug/TJC_TJamSimulator",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "cs_build_release"
        },
        {
            "name": "(Mac) [Debug] TJC_TJamSimulator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cs/bin/TJC_TJamSimulator",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for lldb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ],
            //"miDebuggerPath": "/usr/bin/lldb",
