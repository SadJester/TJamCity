{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Win) [Debug] TJC_TJamSimulator",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cs/bin/Debug/TJC_TJamSimulator",
            "args": [],
