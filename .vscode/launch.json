{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug TJC_TJamSimulator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cs/bin/TJC_TJamSimulator",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
